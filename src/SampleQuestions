---count frequency of each character
#include <stdio.h>
#include <string.h>

void countFrequency(char str[]) {
    int freq[256] = {0};
    for (int i = 0; str[i] != '\0'; i++) {
        unsigned char ch = (unsigned char)str[i];
        freq[ch]++;
    }

    for (int i = 0; i < 256; i++) {
        if (freq[i] > 0) {
            printf("'%c' appears %d times\n", i, freq[i]);
        }
    }
}

int main() {
    char str[] = "invigilation";
    countFrequency(str);
    return 0;
}

