    for(int i=0;i<n;i++){
        if(arr[i]==0){
            temp[j++]=arr[i];
        }
    }
    for(int i=0;i<n;i++){
        if(arr[i]!=0){
            temp[j++]=arr[i];
        }
    }
    for(int i=0;i<n;i++){
        arr[i]=temp[i];
    }
}
int main(){
