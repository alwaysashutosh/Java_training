--missing number by xor
#include<stdio.h>
int missingNo(int arr[], int n){
    int xor1=0;
    int xor2=0;
    for(int i=1;i<=n;i++){
        xor1^=i;
    }
    for(int i=0;i<n-1;i++){
        xor2^=arr[i];
    }
    return xor1^xor2;
}
int main(){
    int arr[]={1,2,3,4,5,6,7,8};
    int n=9;
    int result=missingNo(arr,n);
    printf("%d", result);
    return 0;
}

