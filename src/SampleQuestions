------------Insert an element at a specific position.

#include<stdio.h>
int main(){
    int arr[100];
    int i, n, a, b;
    printf("enter no of elements");
    scanf("%d", &n);
    printf("enter %d elements", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    printf("enter element to insert:");
    scanf("%d", &a);
    printf("enter the index:");
    scanf("%d", &b);
    for(i=0;i<n;i++){
        if(i==b){
            arr[i]=a;
        }
    }
    for(i=0;i<n;i++){
        printf("%d ", arr[i]);
    }
    return 0;
}


-----------Delete an element from a specific position.

#include<stdio.h>
int main(){
    int arr[100];
    int i,n,a;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    printf("enter index to remove: ");
    scanf("%d", &a);
    if(a<0 || a>=n){
        printf("invalid index. \n");
        return 1;
    }
    for(i=a;i<n-1;i++){
        arr[i]=arr[i+1];
    }
    n--;
    for(i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;
}

-------------Shift array elements to the left.

#include<stdio.h>
int main(){
    int arr[100], n, i, first;
    printf("enter no of ele: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    first=arr[0];
    for(i=0; i<n-1;i++){
        arr[i]=arr[i+1];
    }
    arr[n-1]=first;
    for(i=0;i<n;i++){
        printf("%d ", arr[i]);
    }
    return 0;
}

------------------------------------------------

#include<stdio.h>
int main(){
    int arr[100], n, i, pos, j, first;
    printf("enter no of ele: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    printf("enter pos to shift: ");
    scanf("%d", &pos);
    for(j=0; j<pos; j++){
        first=arr[0];
        for(i=0;i<n-1;i++){
            arr[i]=arr[i+1];
        }
        arr[n-1]=first;
    }
    for(i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;

}
----Shift array elements to the right.??????????????????????????????

#include<stdio.h>
int main(){
    int arr[100], i, n, end;
    printf("enter no of ele: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    
    end=arr[n-1];
    for(i=n-1;i>0;i--){
        arr[i]=arr[i-1];
    }
    arr[0]=end;
    for(i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;
}
