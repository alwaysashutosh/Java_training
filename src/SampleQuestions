28. Input a string having both Alpha numeric characters. Print partitioned parts in left and right where left contains the value pushed and right contained all string.???????????????????????

#include<stdio.h>
#include<string.h>
// #include<ctype.h>
#define MAX_LEN 1000

void alphaNum(char* str, int len, char* result){
    char str2[MAX_LEN];
    int j=0, k=0;
    char str3[MAX_LEN];

    for(int i=0; i<len; i++){
        if((str[i]>='A' && str[i]<='Z') || (str[i]>='a' && str[i]<='z')){
            str3[k++]=str[i];
        }else if(str[i]>='0' && str[i]<='9'){
            str2[j++]=str[i];

        }   
    }
    str2[j]='\0';
    str3[k]='\0';
    strcpy(result, str2);
    strcat(result, str3);
}
int main(){
    char str[]="a1b2c3";
    int len=strlen(str);
    char result[MAX_LEN];
    alphaNum(str, len, result);
    printf("result is: %s\n", result);
    return 0;
}

------------------------------------------

#include <stdio.h>
#include <string.h>
#include <ctype.h>  // for isalpha and isdigit

#define MAX_LEN 1000

void alphaNum(char str1[MAX_LEN], int len, char result[MAX_LEN]) {
    char digits[MAX_LEN];
    char letters[MAX_LEN];
    int j = 0, k = 0;

    for (int i = 0; i < len; i++) {
        if (isalpha(str1[i])) {
            letters[k++] = str1[i];
        } else if (isdigit(str1[i])) {
            digits[j++] = str1[i];
        }
    }

    digits[j] = '\0';
    letters[k] = '\0';

    strcpy(result, digits);
    strcat(result, letters);
}

int main() {
    char str1[] = "a1b2c3";
    int len = strlen(str1);
    char result[MAX_LEN];

    alphaNum(str1, len, result);
    printf("Result is: %s\n", result);  // Output: 123abc

    return 0;
}

