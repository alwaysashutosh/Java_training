--count words
#include<stdio.h>
#include<string.h>
#include<ctype.h>
int count(char str[]){
    int count=0;
    for(int i=0;str[i]!='\0';i++){
        if (!isspace(str[i]) && (i == 0 || isspace(str[i - 1]))) {
            count++;
        }

    }
    return count;
}

int main(){
    char str[]=" hello   this is     me  how    about you  sham   ";
    int wordCount=count(str);
    printf("%d\n", wordCount);
    return 0;
}

