---delete all occurrence of x-----
#include<stdio.h>
int removeOc(int arr[], int n, int x){
    for(int i=0;i<n;i++){
        if(arr[i]==x){
            for(int i=0;i<n-1;i++){
                arr[i]=arr[i+1];
            }
            n--;
            i--;
        }
    }
    return n;
}
int main(){
    int arr[]={1,1,2,2,3,4,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    int x=1;
    
    int len=removeOc(arr,n, x);
    for(int i=0;i<len;i++){
        printf("%d", arr[i]);
    }
    return 0;
}

