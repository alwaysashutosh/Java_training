    str[strcspn(str, "\n")]='\0';
    int n=strlen(str);
    int i, j=0;
    for(i=0;i<n;i++){
        if(str[i]!='a' && str[i]!='e' && str[i]!='i' && str[i]!='o' && str[i]!='u'){
            str1[j++]=str[i];
        }
    }
    str1[j]='\0';
    printf("%s\n", str1);
    
    return 0;

}


