-----------------------------------------------------------------------

--merge alternative positive and negative elements

#include<stdio.h>
void alterNum(int arr[], int n){
    int p=0,q=0;
    int arr1[n];
    int arr2[n];
    for(int i=0;i<n;i++){
        if(arr[i]>0){
            arr1[p++]=arr[i];
        }else{
            arr2[q++]=arr[i];
        }
    }
    int i=0,j=0,k=0;
    while(i<p && j<q){
        arr[k++]=arr1[i++];
        arr[k++]=arr2[j++];
    }
    
    while (i < p)
        arr[k++] = arr1[i++];
    while (j < q)
        arr[k++] = arr2[j++];
}

int main(){
    int arr[]={1,0,2,0};
    int n=sizeof(arr)/sizeof(arr[0]);
    moveZero(arr,n);
    for(int i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;
}

