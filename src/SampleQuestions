---move zero to end
#include<stdio.h>
void moveZero(int arr[], int n){
    int temp[n];
    int j=0;
    for(int i=0;i<n;i++){
        if(arr[i]!=0){
            temp[j++]=arr[i]
        }
    }
    for(int i=j;i<n;i++){
        temp[j++]=0;
    }
    for(int i=0;i<n;i++){
        arr[i]=temp[i];
    }
}
int main(){
    int arr[]={1,0,2,0};
    int n=sizeof(arr)/sizeof(arr[0]);
    moveZero(arr,n);
    for(int i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;

}

