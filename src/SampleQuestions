//expand string

#include<stdio.h>
#include<string.h>
#include<ctype.h>

void firstRepeat(char str[]){
    char str1[100];
    int j=0;
    for(int i=0;str[i]!='\0';i++){
        if(isalpha(str[i]) && isdigit(str[i+1])){
            int digit=str[i+1]-'0';
            for(int k=0;k<digit;k++){
                str1[j++]=str[i];
            }
        }
    }
    str1[j]='\0';
    printf("%s", str1);
}
int main(){
    char str[]="a3b2";
    firstRepeat(str);
    return 0;
}



--------------------swap adjacent pairs-------------------------------------------
----------------------------------------------------------------------------

#include<stdio.h>
void nonZero(int arr[], int n){
    for(int i=0;i<n-1;i+=2){
        int temp=arr[i];
        arr[i]=arr[i+1];
        arr[i+1]=temp;
    }
    for(int i=0;i<n;i++){
        printf("%d", arr[i]);
    }

}
int main(){
    int arr[]={1,2,3,4,5};
    int n=sizeof(arr)/sizeof(arr[0]);
    nonZero(arr,n);
    return 0;
}


