---2 repeating element in the array of size n+2.

#include<stdio.h>//efficient
void findRep(int arr[], int n){
    int freq[n+1];
    for(int i=0;i<=n;i++){
        freq[i]=0;
    }
    for(int i=0;i<n+2;i++){
        if(freq[arr[i]]==1){
            printf("%d", arr[i]);
        }else{
            freq[arr[i]]++;
        }
    }
}


int main(){
    int arr[]={1,2,3,4,5,6,5,4};
    int n=6;
    findRep(arr,n);
    return 0;
}

