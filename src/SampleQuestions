#include<stdio.h>
#include<string.h>

void dupStr(char str[], char result[]){
    char seen[275]={0};
    int k=0;
    for(int i=0;str[i]='\0';i++){
        unsigned char ch=(unsigned char) str[i];
        if(!seen[ch]){
            seen[ch]=1;
            result[k++]=str[i];
        }
    }
    result[k]='\0';
}
int main(){
    char str[]="invigilation";
    int n=strlen(str);
    char result[100];
    dupStr(str, n, result);
    printf("%s\n", result);
    return 0;
}

