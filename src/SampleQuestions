--partition array
#include<stdio.h>
void partitionArr(int arr[], int n){
    int temp[n];
    int j=0;
    for(int i=0;i<n;i++){
        if(arr[i]%2==0){
            temp[j++]=arr[i];
        }
    }
    for(int i=0;i<n;i++){
        if(arr[i]%2!=0){
            temp[j++]=arr[i];
        }
    }
    for(int i=0;i<n;i++){
        arr[i]=temp[i];
    }
}
int main(){
    int arr[]={2,3,4,5,6,7,8};
    int n=sizeof(arr)/sizeof(arr[0]);
    partitionArr(arr,n);
    for(int i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;
}

