----Implement binary search on a sorted array

#include<stdio.h>
int binarySearch(int arr[], int n, int key){
    int low=0, high=n-1;
    while(low<=high){
        int mid=(low+high)/2;
        if(arr[mid]==key){
            return mid;
        }
        else if(arr[mid]<key){
            low=mid+1;
        }
        else{
            high=mid-1;
        }
    }
    return -1;
}
int main(){
    int arr[]={2,5,8,12,16};
    int n=sizeof(arr)/sizeof(arr[0]);
    int key=12;
    int result=binarySearch(arr, n, key);
    if(result!=-1){
        printf("%d", result);
    }else{
        printf("not found");
    }
    return 0;
}

-----Find the peak element in an array
#include<stdio.h>
int findPeak(int arr[], int n){
    if(n==1){
        return arr[0];
    }
    if(arr[0]>=arr[1]){
        return arr[0];
    }
    for(int i=1; i<n-1;i++){
        if(arr[i]>=arr[i-1] && arr[i]>=arr[i+1]){
            return arr[i];
        }
    }
    return arr[n-1];
}
int main(){
    int arr[]={1,3,20,4,1};
    int n=sizeof(arr)/sizeof(arr[0]);
    int peak=findPeak(arr,n);
    printf("%d", peak);
    return 0;
}


----largest and second largest element in an array.

#include<stdio.h>
int main(){
    int arr[100], n, i;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ");
    scanf("%d", &arr[i]);
    int largest, second;
    if(n<2){
        printf("must have two elements.\n");
        return 1;
    }
    if(arr[0]>arr[1]){
        largest=arr[0];
        second=arr[1];
    }else{
        largest=arr[1];
        second=arr[0];
    }
    for(i=2;i<n;i++){
        if(arr[i]>largest){
            second=largest;
            largest=arr[i];
        }
        else if (arr[i]>second && arr[i]!=largest){
            second=arr[i];
        }
    }
    printf("%d %d", largest, second);
    return 0;
}
----Find the maximum and minimum element in an array.

#include<stdio.h>
int main(){
    int arr[100], min, max, i, n;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter %d elements: ", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    min=arr[0];
    max=arr[0];
    for(i=1;i<n;i++){
        if(arr[i]>max){
            max=arr[i];
        }
        if(arr[i]<min){
            min=arr[i];
        }
    }
    printf("%d %d", max, min);
    return 0;
}

----Sort an array in ascending order (bubble sort).

#include<stdio.h>
int main(){
    int arr[100], n,i, j;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0; i<n;i++){
        scanf("%d", &arr[i]);
    }
    for(i=0;i<n-1;i++){
        int temp;
        for(j=0;j<n-i-1;j++){
            if(arr[j]>arr[j+1]){
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
    printf("sorted elements: ");
    for(i=0;i<n;i++){
        printf("%d ", arr[i]);
    }
    return 0;

}
----Sort an array in descending order.

#include<stdio.h>
int main(){
    int arr[100], n,i, j;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0; i<n;i++){
        scanf("%d", &arr[i]);
    }
    for(i=0;i<n-1;i++){
        int temp;
        for(j=0;j<n-i-1;j++){
            if(arr[j]<arr[j+1]){
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
    printf("sorted elements: ");
    for(i=0;i<n;i++){
        printf("%d ", arr[i]);
    }
    return 0;

}

----Copy one array to another.

#include<stdio.h>
int main(){
    int arr1[100], n1, i, arr2[100], j=0;
    printf("enter no of elements: ");
    scanf("%d", &n1);
    printf("enter %d elements: ", n);
    for(i=0;i<n1;i++){
        scanf("%d", &arr1[i]);
    }
    for(i=0;i<n1;i++){
        arr2[j++]=arr1[i];
    }
    for(i=0;i<n1; i++){
        printf("%d", arr2[i]);
    }
    return 0;
}

---Merge two arrays into a third array

#include<stdio.h>
int main(){
    int arr1[100];
    int arr2[100];
    int n1, n2, i;
    printf("enter no of elements of arr1: ");
    scanf("%d", &n1);
    printf("enter %d elements: ", n1);
    for(i=0;i<n1;i++){
        scanf("%d", &arr1[i]);
    }
    printf("enter no of elements of arr2: ");
    scanf("%d", &n2);
    printf("enter %d elements: ", n2);
    for(i=0;i<n2;i++){
        scanf("%d", &arr2[i]);
    }
    int arr3[n1+n2];
    int j=0;
    for(i=0;i<n1;i++){
        arr3[j++]=arr1[i];
    }
    for(i=0;i<n2;i++){
        arr3[j++]=arr2[i];
    }
    printf("merged elements: ");
    for(i=0; i<n1+n2;i++){
        printf("%d", arr3[i]);
    }
    return 0;
}

#include<stdio.h>
int main(){
    int arr1[100];
    int arr2[100];
    int n1, n2, i;
    printf("enter no of elements of arr1: ");
    scanf("%d", &n1);
    printf("enter %d elements: ", n1);
    for(i=0;i<n1;i++){
        scanf("%d", &arr1[i]);
    }
    printf("enter no of elements of arr2: ");
    scanf("%d", &n2);
    printf("enter %d elements: ", n2);
    for(i=0;i<n2;i++){
        scanf("%d", &arr2[i]);
    }
    for(i=0;i<n2;i++){
        arr1[n1+i]=arr2[i];
    }
    for(i=0; i<n1+n2;i++){
        printf("%d", arr1[i]);
    }
    return 0;
}

