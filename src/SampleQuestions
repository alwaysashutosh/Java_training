------Reverse a string without using library functions.

#include<stdio.h>
#include<string.h>
void reverse(char str[]){
    int start=0;
    int end=strlen(str)-1;
    char temp;
    while(start<end){
        temp=str[start];
        str[start]=str[end];
        str[end]=temp;
        start++;
        end--;
    }
}

int main(){
    char str[100];
    printf("enter string: ");
    scanf("%s", str);
    reverse(str);
    printf("%s", str);
    return 0;
}

