---intersection of two arrays

#include<stdio.h>
void intersection(int arr1[], int arr2[], int size1, int size2){
    int temp[100];
    int k=0;
    for(int i=0;i<size1;i++){
        for(int j=0;j<size2;j++){
            if(arr1[i]==arr2[j]){
                temp[k++]=arr1[i];
                break;
            }
        }
    }
    int freq[256]={0};
    int j=0;
    int result[100];
    for(int i=0;i<k;i++){
        if(freq[temp[i]]==0){
            result[j++]=temp[i];
            freq[temp[i]]=1;
        }
    }
    for(int i=0;i<j;i++){
        printf("%d", result[i]);
    }
    printf("\n");
}
int main(){
    int arr1[]={1,2,3,4,5,5};
    int arr2[]={3,4,5,6};
    int size1=sizeof(arr1)/sizeof(arr1[0]);
    int size2=sizeof(arr2)/sizeof(arr2[0]);
    int arr[100];
    intersection(arr1,arr2,size1,size2);
    return 0;
}


