---Replace spaces with hyphens.

#include<stdio.h>
#include<string.h>

void replace(char str[100], char old, char new){
    for(int i=0; str[i]!='\0'; i++){
        if(str[i]==old){
            str[i]=new;
        }
    }
}
int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    replace(str, ' ', '-');
    printf("%s", str);
    return 0;
}

void replace(char *str, char old, char new){
    for(char *p=str; *p!='\0'; p++){
        if(*p==old){
            *p=new;
        }
    }
}

----Count the number of words in a string.

#include<stdio.h>
#include<string.h>
#include<ctype.h>

int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    
    int i, inWord=0, count=0;
    for(i=0; str[i]!='\0';i++){
        if(!isspace(str[i]) && inWord==0){
            inWord=1;
            count++;
        }else if(isspace(str[i])){
            inWord=0;
        }
    }
    printf("%d", count);
    return 0;
}


----Find the longest word in a string.

#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    char *token=strtok(str, " ");
    char *longest=token;

    while(token!=NULL){
        if(strlen(token)>strlen(longest)){
            longest=token;
        }
        token=strtok(NULL, " ");
    }
    printf("longest word: %s\n", longest);
    printf("length: %lu\n", strlen(longest));
    return 0;
}

----Find the shortest word in a string.

#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    char *token=strtok(str, " ");
    char *shortest=token;
    while(token != NULL){
        if(strlen(token)<strlen(shortest)){
            shortest=token;
        }
        token=strtok(NULL, " ");
    }
    printf("Shortest word: %s\n", shortest);
    printf("length: %lu\n", strlen(shortest));
    return 0;
}


----Check if one string is a substring of another.
#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    char str1[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';

    printf("enter string to find: ");
    fgets(str1, sizeof(str1), stdin);
    str1[strcspn(str1, "\n")]='\0';

    if(strstr(str, str1)!=NULL){
        printf("%s is substring of %s\n", str1, str);
    }else{
        printf("%s is not a substring of %s\n", str1, str);
    }
    return 0;
}



----Toggle the case of each character.

#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';

    for(int i=0; str[i]!='\0'; i++){
        if(isupper(str[i])){
            str[i]=tolower(str[i]);
        }
        else if(islower(str[i])){
            str[i]=toupper(str[i]);
        }
    }
    printf("toggled string: %s\n", str);
    return 0;
}

#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    for(int i=0;str[i]!='\0';i++){
        if(islower(str[i])){
            str[i]=toupper(str[i]);
        }
        else if(isupper(str[i])){
            str[i]=tolower(str[i]);
        }
    }
    printf("toggled string: %s\n", str);
    return 0;

}

----Sort characters of a string alphabetically.

#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    int n=strlen(str);
    char temp;

    for(int i=0; i<n-1;i++){
        for(int j=0;j<n-i-1;j++){
            if(str[j]>str[j+1]){
                temp=str[j];
                str[j]=str[j+1];
                str[j+1]=temp;
            }
        }
    }
    printf("sorted string: %s\n", str);
    return 0;
}


