--max length of subarray whose sum is k
#include <stdio.h>

#define MAX 1000
#define OFFSET 500  // To handle negative prefix sums

void maxSubLen(int arr[], int n) {
    int arr1[n];
    for (int i = 0; i < n; i++) {
        arr1[i] = (arr[i] == 0) ? -1 : arr[i];
    }

    int prefixSum[n];
    prefixSum[0] = arr1[0];
    for (int i = 1; i < n; i++) {
        prefixSum[i] = prefixSum[i - 1] + arr1[i];
    }

    int firstIndex[MAX];
    for (int i = 0; i < MAX; i++) {
        firstIndex[i] = -1;
    }

    int maxLen = 0;

    for (int i = 0; i < n; i++) {
        int sum = prefixSum[i];

        if (sum == 0) {
            maxLen = i + 1;  // Subarray from start to i
        }

        int index = sum + OFFSET;
        if (firstIndex[index] != -1) {
            int len = i - firstIndex[index];
            if (len > maxLen) {
                maxLen = len;
            }
        } else {
            firstIndex[index] = i;  // Store first occurrence
        }
    }

    printf("Maximum length of subarray with sum 0 is: %d\n", maxLen);
}

int main() {
    int arr[] = {0, 0, 0, 0, 1, 1};
    int n = sizeof(arr) / sizeof(arr[0]);
    maxSubLen(arr, n);
    return 0;
}


