    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")] = '\0'; 
    int n=strlen(str);
    int i, inword=0, count=0;
    for(i=0;i<n;i++){
        if(!isspace(str[i]) && inword=0){
