#include<stdio.h>
void removeDup(int arr[], int size){
    int freq[256]={0}; 
    int i =0,j=0;
    for(int i=0;i<size;i++){
        if(freq[arr[i]]==0){
            freq[j++]=arr[i];
            freq[arr[i]]=1;
        }
    }
    for(int i=0;i<j;i++){
        printf("%d", arr[i]);
    }
}
    
int main(){
    int arr[]={1,2,3,2,3};
    int size=sizeof(arr)/sizeof(arr[0]);
    removeDup(arr,size);
    return 0;
}



#include<stdio.h>
#include<string.h>
#include<ctype.h>

void capitalize(char str[]){
    int n;
    printf("%d",n);
    if(!isspace(str[0])){
        str[0]=toupper(str[0]);
    }
    for(int i=1;i<n;i++){
        if(isspace(str[i-1])){
            str[i]=toupper(str[i]);
        }
    }
    
}
int main(){
    char str[1000];
    int n=strlen(str);
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    capitalize(str);
    printf("%s", str);
    return 0;
}


--------------remove duplicates two approach----------------------
------------------------------------------------------------------
#include<stdio.h>
int removeDup(int arr[], int n){
    if(n==0)return 0;
    int temp[n];
    int j;
    for(int i=0;i<n-1;i++){
        if(arr[i]!=arr[i+1]){
            temp[j++]=arr[i];
        }
    }
    temp[j++]=arr[i];
    for(int i=0;i<j;i++){
        arr[i]=temp[i];
    }
    return j;
}
int main(){
    int arr[]={1,1,2,2,3,4,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    int newL=removeDup(arr,n);
    for(int i=0;i<newL;i++){
        printf("%d", arr[i]);
    }
    return 0;
}

////or

#include<stdio.h>
int removeDup(int arr[], int n){
    if(n==0) return 0;
    int i=0;
    for(int j=1;j<n;j++){
        if(arr[i]!=arr[j]){
            i++;
            arr[i]=arr[j];
        }
    }
    return i+1;
}
int main(){
    int arr[]={1,1,2,2,3,4,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    int newL=removeDup(arr,n);
    for(int i=0;i<newL;i++){
        printf("%d", arr[i]);
    }
    return 0;
}

