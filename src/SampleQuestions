----Compare two strings without using strcmp().

#include<stdio.h>
int main(){
    char str1[100], str2[100];
    printf("enter first string: ");
    scanf("%s", str1);
    printf("enter second string: ");
    scanf("%s", str2);
    int i=0, flag=0;
    while(str1[i]!='\0' || str2[i]!='\0'){
        if(str1[i]!=str2[i]){
            flag=1;
            break;
        }
        i++;
    }
    if(flag==0){
        printf("equal\n");
    }else{
        printf("not equal\n");
    }
    return 0;
}
---Concatenate two strings without using strcat().
strcat(str1, str2)
#include<stdio.h>
int main(){
    char str1[1000], str2[1000];
    printf("enter str1: ");
    scanf("%s", str1);
    printf("enter str2: ");
    scanf("%s", str2);
    int i=0, j=0;
    while(str1[i]!='\0'){
        i++;
    }
    while(str2[j]!='\0'){
        str1[i++]=str2[j++];
    }
    str1[i]='\0';
    printf("string %s\n", str1);
    return 0;
}


