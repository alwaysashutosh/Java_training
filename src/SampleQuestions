#include<stdio.h>///not efficient
void findRep(int arr[], int n){
    int temp[n];
    int k=0;
    for(int i=0;i<n+2;i++){
        for(int j=i+1;j<n+2;j++){
            if(arr[i]==arr[j]){
                temp[k++]=arr[j];
            }
        }
    }
    for(int i=0;i<k;i++){
        printf("%d\n", temp[i]);
    }
}
int main(){
    int arr[]={1,2,3,4,5,6,5,4};
    int n=6;
    findRep(arr,n);
    return 0;
}


