-----Count frequency of each character.

#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    char freq[100];
    int i,j,count;
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    int n=strlen(str);
    for(i=0;i<n;i++){
        freq[i]=-1;
    }
    for(i=0; i<n; i++){
        int count=1;
        for(j=i+1; j<n;j++){
            if(str[i]==str[j]){
                count++;
                freq[j]=0;
            }
        }
        if(freq[i]!=0){
            freq[i]=count;
        }

    }
    printf("frequency of each element: \n");
    for(i=0;i<n;i++){
        if(freq[i]!=0){
            printf("%c occurs %d times\n", str[i], freq[i]);
        }
    }
    return 0;
}


#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    char freq[100];
    int i,j,count;
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    int n=strlen(str);
    for(i=0;i<n;i++){
        freq[i]=-1;
    }
    for(i=0;i<n;i++){
        count=1;
        for(j=i+1;j<n;j++){
            if(str[i]==str[j]){
                count++;
                freq[j]=0;
            }
        }
        if(freq[i]!=0){
            freq[i]=count;
        }
    }
    printf("frequency of each element: \n");
    for(i=0;i<n;i++){
        if(freq[i]!=0){
            printf("%c occurs %d times\n", str[i], freq[i]);
        }
    }
    return 0;
}





