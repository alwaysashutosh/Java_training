17-08-25

Basic Level
----Reverse an array

#include<stdio.h>
int main(){
    int arr[100], n, i;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    int temp;
    int start=0;
    int end=n-1;
    while(start<end){
        temp=arr[start];
        arr[start]=arr[end];
        arr[end]=temp;
        start++;
        end--;
    }
    for(i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;
    
}

-----Check if an array is sorted
#include <stdio.h>
int main() {
    int arr[100], n, i, isSorted = 1;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter %d elements: ", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    for(i = 0; i < n - 1; i++) {
        if(arr[i] > arr[i + 1]) {
            isSorted = 0;
            break;
        }
    }
    if(isSorted) {
        printf("The array is sorted in ascending order.\n");
    } else {
        printf("The array is NOT sorted.\n");
    }
    return 0;
}


ðŸ”¹ Intermediate Level
----------------Left rotate or right rotate an array by k positions

#include<stdio.h>
void leftRotate(int arr[], int n, int d){
    int temp[d], i;
    for(i=0;i<d;i++){
        temp[i]=arr[i];
    }
    for(i=d;i<n;i++){
        arr[i-d]=arr[i];
    }
    for(i=0;i<d;i++){
        arr[n-d+i]=temp[i];
    }
}
int main(){
    int arr[100], n,i, d;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ");
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    printf("enter position: ");
    scanf("%d", &d);
    leftRotate(arr, n, d);
    for(int i=0; i<n;i++){
        printf("%d", arr[i]);
    }
    printf("\n");
    return 0;
}

////////////////////////////////////////////////////////////

#include<stdio.h>
void rightRotate(int arr[], int n, int d){
    int temp[d];
    for(int i=0;i<d;i++){
        temp[i]=arr[n-d+i];
    }
    for(int i=n-d-1;i>=0;i--){
        arr[i+d]=arr[i];
    }
    for(int i=0;i<d;i++){
        arr[i]=temp[i];
    }
}
int main(){
    int arr[]={1,2,3,4,5};
    int n=sizeof(arr)/sizeof(arr[0]);
    int d=2;
    rightRotate(arr, n, d);
    for(int i=0;i<n;i++){
        printd("%d", arr[i]);
    }
    printf("\n");
    return 0;
}


-----Find the missing number in a sequence

#include <stdio.h>

int main() {
    int arr[100], n, i, total, sum = 0;
    printf("Enter the value of n (total numbers including the missing one): ");
    scanf("%d", &n);
    printf("Enter %d elements (from 1 to %d, with one missing):\n", n - 1, n);
    for(i = 0; i < n - 1; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }
    total = n * (n + 1) / 2;
    int missing = total - sum;
    printf("The missing number is: %d\n", missing);
    return 0;
}

-----Find the intersection or union of two arrays

#include<stdio.h>
void intersection(int arr1[], int n1, int arr2[], int n2){
    int i,j;
    for(i=0;i<n1;i++){
        for(j=0;j<n2;j++){
            if(arr1[i]==arr2[j]){
                printf("%d", arr1[i]);
            }
        }
    }
    printf("\n");
}

void unionArr(int arr1[], int n1, int arr2[], int n2){
    int i,j;
    for(i=0;i<n1;i++){
        printf("%d", arr1[i]);
    }
    for(i=0;i<n2;i++){
        int found=0;
        for(j=0;j<n1;j++){
            if(arr2[i]==arr1[j]){
                found=1;
                break;
            }
        }
        if(!found){
            printf("%d", arr2[i]);
        }
    }
    printf("\n");
}
int main(){
    int arr1[]={1,2,3,4};
    int arr2[]={3,4,5,6};
    int n1=sizeof(arr1)/sizeof(arr1[0]);
    int n2=sizeof(arr2)/sizeof(arr2[0]);
    unionArr(arr1, n1, arr2, n2);
    intersection(arr1, n1, arr2, n2);
    return 0;
}

----Move all zeros to the end of the array

#include<stdio.h>
void moveZero(int arr[], int n){
    int count=0;
    for(int i=0;i<n;i++){
        if(arr[i]!=0){
            arr[count++]=arr[i];
        }
    }
    while(count<n){
        arr[count++]=0;
    }
}
int main(){
    int arr[]={1,0,2,0,3};
    int n=sizeof(arr)/sizeof(arr[0]);
    moveZero(arr,n);
    for(int i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    printf("\n");
    return 0;
}

---Find the pair with a given sum

#include<stdio.h>
void findPair(int arr[], int n, int sum){
    int found=0;
    for(int i=0;i<n-1;i++){
        for(int j=i+1;j<n;j++){
            if(arr[i]+arr[j]==sum){
                printf("%d %d\n", arr[i], arr[j]);
                found=1;
            }
        }
    }
    if(!found){
        printf("no pair \n");
    }
}
int main(){
    int arr[]={8,7,2,5};
    int n=sizeof(arr)/sizeof(arr[0]);
    int sum=7;
    findPair(arr, n, sum);
    return 0;
}


