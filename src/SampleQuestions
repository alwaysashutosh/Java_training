#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    int len, i;
    char temp;
    printf("enter str: ");
    scanf("%s", str);
    len=strlen(str);
    for(i=0;i<len/2;i++){
        temp=str[i];
        str[i]=str[len-i-1];
        str[len-i-1]=temp;
    }
    printf("%s\n", str);
    return 0;

}

///////////////////////////////////////////////////////////reverse individual word

#include<stdio.h>
#include<string.h>

void reverse(char str[], int start, int end){
    char temp;
    while(start<end){
        temp=str[start];
        str[start]=str[end];
        str[end]=temp;
        start++;
        end--;
    }
}

int main(){
    char str[100];
    int start=0, i=0;
    printf("enter sentence: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    while(str[i] != '\0'){
        if(str[i]==' '){
            reverse(str, start, i-1);
            start=i+1;
        }
        i++;
    }
    reverse(str, start, i-1);
    printf("%s", str);
    return 0;
}



------------------Check if a string is a palindrome.

#include <stdio.h>
#include <string.h>
#include <stdbool.h>

bool isPalin(char str[]) {
    int n = strlen(str);
    int start = 0, end = n - 1;

    while (start < end) {
        if (str[start] != str[end]) {
            return false;
        }
        start++;
        end--;
    }
    return true;
}

int main() {
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")] = '\0';

    if (isPalin(str)) {
        printf("Palindrome\n");
    } else {
        printf("Not a palindrome\n");
    }

    return 0;
}

------Count vowels and consonants in a string.
#include<stdio.h>
#include<string.h>
int main(){
    int vowel=0, cons=0, i;
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    for(i=0;i<strlen(str); i++){
        if(str[i]=='a' || str[i]=='e' || str[i]=='i' || str[i]== 'o' || str[i]== 'u'){
            vowel+=1;
        }else{
            cons+=1;
        }
    }
    printf("Vowels: %d, Cons: %d", vowel, cons);
    return 0;

}


----Convert a string to uppercase.

#include<stdio.h>
#include<string.h>
#include<ctype.h>
int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    for(int i=0; i<strlen(str); i++){  //for(int i=0; str[i]!='\0'; i++)
        str[i]=toupper(str[i]);         // L U -32
    }
    printf("uppercase: %s\n", str);
    return 0;
}

