#include<stdio.h>
int main(){
    int arr[100], n, num;
    printf("enter number of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(int i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    printf("enter num: ");
    scanf("%d", &num);
    for(int i=0;i<n;i++){
        for(j=i+1;j<n;j++){
            if(arr[i]+arr[j]==num){
                printf("%d, %d", arr[i], arr[j]);
            }
        }
    }
    return 0;
}

