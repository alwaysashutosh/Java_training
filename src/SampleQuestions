--------------return true if array has any unique frequency

#include<stdio.h>
#include<stdbool.h>

bool intersection(int arr[], int size){
    int freq[256]={0};
    for(int i=0;i<size;i++){
        freq[arr[i]]++;
    }
    int freqCount[256]={0};
    for(int i=0;i<256;i++){
        if(freq[i]>0){
            if(freqCount[freq[i]]!=0){
                return false;
            }
            freqCount[freq[i]]=1;
        }
    }
    return true;
}
int main(){
    int arr[]={1,2,2,4,4};
    int size=sizeof(arr)/sizeof(arr[0]);
    intersection(arr, size);
    if(intersection(arr,size)){
        printf("true");
    }else{
        printf("false");
    }
    return 0;
}


