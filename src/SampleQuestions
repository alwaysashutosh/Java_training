--first unique char in string
#include<stdio.h>
void firstUnique(char str[]){
    int freq[256]={0};
    int j=0;
    for(int i=0; str[i]!='\0'; i++){
        freq[str[i]]++;
    }
    for(int i=0;i<256;i++){
        if(freq[str[i]]==1){
            printf("%c", str[i]);
            break;
        }
    }
}
int main(){
    char str[]="aafbcdd";
    firstUnique(str);
    return 0;
}

