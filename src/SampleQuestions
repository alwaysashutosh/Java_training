--Calculate the average of array elements.

#include<stdio.h>
int average(int arr[], int size){
    int sum=0;
    int average=0;
    for(int i=0; i<size; i++){
        sum+=arr[i];
        average=sum/size;
    }
    return average;
}
int main(){
    int arr[]={1,2,3,4,5};
    int size=sizeof(arr)/sizeof(arr[0]);
    int result=average(arr, size);
    printf("%d\n", result);
    return 0;
}

--Find the maximum and minimum element in an array.???????????????????????????????????????????????

#include <stdio.h>

void maxMin(int arr[], int size, int *max, int *min) {
    *max = arr[0];
    *min = arr[0];
    for (int i = 1; i < size; i++) {
        if (arr[i] > *max) {
            *max = arr[i];
        }
        if (arr[i] < *min) {
            *min = arr[i];
        }
    }
}

int main() {
    int arr[] = {3, 1, 5, 4};
    int size = sizeof(arr) / sizeof(arr[0]);
    int max, min;
    maxMin(arr, size, &max, &min);
    printf("Max: %d, Min: %d\n", max, min);
    return 0;
}



-----Reverse the array.

#include<stdio.h>
void reverseArr(int arr[], int size){
    int start=0;
    int end=size-1;
    while(start<end){
        int temp=arr[start];
        arr[start]=arr[end];
        arr[end]=temp;
        start++;
        end--;
    }
}

int main(){
    int arr[]={2,3,4,5,6};
    int size=sizeof(arr)/sizeof(arr[0]);
    reverseArr(arr, size);
    for(int i=0; i<size;i++){
        printf("%d\n", arr[i]);
    }
    return 0;
}


---------------------------------------------------

#include<stdio.h>
void reverse(int arr[], int size){
    int start=0, end=size-1, temp;
    while(start<end){
        temp=arr[start];
        arr[start]=arr[end];
        arr[end]=temp;
        start++;
        end--;
    }
}
int main(){
    int arr[100], n, i, j;
    printf("enter no of ele:  ");
    scanf("%d", &n);
    printf("enter %d ele", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    reverse(arr,n);
    for(i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;
}

----Count even and odd numbers in an array. 11111111111111111111111111111111111

#include<stdio.h>
int evenOdd(int arr[], int size, int *evenCount, int *oddCount){
    *evenCount=0;
    *oddCount=0;
    for(int i=0; i<size; i++){
        if(arr[i]%2==0){
            *evenCount+=1;
        }else{
            *oddCount+=1;
        }
    }
}
int main(){
    int arr[]={1,2,3,4,5};
    int size=sizeof(arr)/sizeof(arr[0]);
    int evenCount, oddCount;
    evenOdd(arr, size, &evenCount, &oddCount);
    printf("even count %d odd count %d\n", evenCount, oddCount);
    return 0;
}

------Search for an element using linear search.

#include <stdio.h>

int linearSearch(int arr[], int size, int key) {
    for (int i = 0; i < size; i++) {
        if (arr[i] == key) {
            return i; // Return the index where the key is found
        }
    }
    return -1; // Return -1 if the key is not found
}

int main() {
    int arr[] = {3, 1, 5, 4, 9};
    int size = sizeof(arr) / sizeof(arr[0]);
    int key = 5;

    int result = linearSearch(arr, size, key);

    if (result != -1) {
        printf("Element %d found at index %d.\n", key, result);
    } else {
        printf("Element %d not found in the array.\n", key);
    }

    return 0;
}

------Sort an array in ascending order.

#include<stdio.h>
void sort(int arr[], int size){
    for(int i=0; i<size-1; i++){
        for(int j=i+1; j<size; j++){
            if(arr[i]>arr[j]){
                int temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
            }
        }
    }
}

int main(){
    int arr[]={8,1,9,3,4,6,2};
    int size=sizeof(arr)/sizeof(arr[0]);
    sort(arr,size);
    for(int i=0; i<size; i++){
        printf("%d\n", arr[i]);
    }
    return 0;
}
-----Sort an array in descending order.

#include<stdio.h>
void sort(int arr[], int size){
    for(int i=0; i<size; i++){
        for(int j=i+1; j<size; j++){
            if(arr[i]<arr[j]){
                int temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
            }
        }
    }
}

int main(){
    int arr[]={8,1,9,3,4,6,2};
    int size=sizeof(arr)/sizeof(arr[0]);
    sort(arr,size);
    for(int i=0; i<size; i++){
        printf("%d\n", arr[i]);
    }
    return 0;
}
------Copy one array to another.

#include<stdio.h>
int main(){
    int arr[]={1,2,3,4,5};
    int size=sizeof(arr)/sizeof(arr[0]);
    int copy[size];
    for(int i=0; i<size; i++){
        copy[i]=arr[i];
    }
    printf("copied array: \n");
    for(int i=0; i<size; i++){
        printf("%d\n", copy[i]);
    }
    return 0;
}

-----Merge two arrays into a third array.

#include <stdio.h>

int main() {
    int arr1[] = {1, 3, 5};
    int arr2[] = {2, 4, 6, 8};
    int size1 = sizeof(arr1) / sizeof(arr1[0]);
    int size2 = sizeof(arr2) / sizeof(arr2[0]);
    int merged[size1 + size2];

    // Copy elements from arr1
    for (int i = 0; i < size1; i++) {
        merged[i] = arr1[i];
    }

    // Copy elements from arr2
    for (int i = 0; i < size2; i++) {
        merged[size1 + i] = arr2[i];
    }

    // Display merged array
    printf("Merged array: ");
    for (int i = 0; i < size1 + size2; i++) {
        printf("%d ", merged[i]);
    }

    return 0;
}

------Find the second largest element in an array.
#include<stdio.h>
#include<limits.h>

int secondLar(int arr[], int size){
    int lar=INT_MIN;
    int sec=INT_MIN;
    for(int i=0; i<size; i++){
        if(arr[i]>lar){
            sec=lar;
            lar=arr[i];
        }else if(arr[i]>sec && arr[i]!=lar){
            sec=arr[i];
        }
    }
    return second;
}


int main() {
    int arr[] = {3, 1, 5, 4, 9};
    int size = sizeof(arr) / sizeof(arr[0]);

    int secondLargest = findSecondLargest(arr, size);

    if (secondLargest != INT_MIN) {
        printf("Second largest element is: %d\n", secondLargest);
    }
    return 0;
}



-----Count frequency of each element. ?????????????????????????????//111

#include<stdio.h>
int main(){
    int arr[100], freq[100];
    int i,j,n,count;
    printf("enter no of elements: \n");
    scanf("%d\n", &n);
    printf("enter %d elements: ", n);
    for(int i=0; i<n;i++){
        scanf("%d", &arr[i]);
        freq[i]=-1;
    }
    for(i=0; i<n;i++){
        int count=1;
        for(j=i+1; j<n;j++){
            if(arr[i]==arr[j]){
                count++;
                freq[j]=0;
            }
        }
        if(freq[i]!=0){
            freq[i]=count;
        }
    }
    printf("frequency of each element: \n");
    for(i=0;i<n;i++){
        if(freq[i]!=0){
            printf("%d occurs %d times\n", arr[i], freq[i]);
        }
    }
    return 0;
}


#include <stdio.h>

int main() {
    int arr[100], freq[1000] = {0}; // assuming elements are in range 0â€“999
    int n;

    printf("Enter number of elements:\n");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        freq[arr[i]]++; // count frequency directly
    }

    printf("Frequency of each element:\n");
    int printed[1000] = {0}; // to avoid duplicate printing
    for (int i = 0; i < n; i++) {
        if (!printed[arr[i]]) {
            printf("%d occurs %d times\n", arr[i], freq[arr[i]]);
            printed[arr[i]] = 1;
        }
    }

    return 0;
}


#include<stdio.h>
int main(){
    int arr[100], freq[100];
    int n,i,j,count;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0; i<n;i++){
        scanf("%d", &arr[i]);
        freq[i]=-1;
    }
    for(i=0; i<n;i++){
        count=1;
        for(j=i+1;j<n;j++){
            if(arr[i]==arr[j]){
                count++;
                freq[j]=0;
            }
        }
        if(freq[i]!=0){
            freq[i]=count;
        }
    }
    for(i=0;i<n;i++){
        if(freq[i]!=0){
            printf("%d occurs %d times", arr[i], freq[i]);
        }
    }
    return 0;
}

------------Remove duplicate elements from an array.???????????222

#include<stdio.h>
int main(){
    int arr[100], freq[100], res[100];
    int n,i,j,count, index=0;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
        freq[i]=-1;
    }
    for(i=0;i<n;i++){
        count=1;
        for(j=i+1;j<n;j++){
            if(arr[i]==arr[j]){
                count++;
                freq[j]=0;
            }
        }
        if(freq[i]!=0){
            freq[i]=count;
        }
    }
    for(i=0;i<n;i++){
        if(freq[i]==1){
            res[index++]=arr[i];
        }
    }
    printf("non duplicate elements: \n");
    for(i=0;i<index;i++){
        printf("%d", res[i]);
    }

    return 0;
}


#include<stdio.h>
int main(){
    int arr[100], freq[100], res[100];
    int n, i, j, count, index=0;
    printf("enter no of elements: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
        freq[i]=-1;
    }
    for(i=0;i<n;i++){
        count=1;
        for(j=i+1;j<n;j++){
            if(arr[i]==arr[j]){
                count++;
                freq[j]=0;
            }
        }
        if(freq[i]!=0){
            freq[i]=count;
        }
    }
    for(i=0;i<n;i++){
        if(freq[i]==1){
            res[index++]=arr[i];
        }
    }
    for(i=0;i<index;i++){
        printf("%d", res[i]);
    }
    return 0;

}


