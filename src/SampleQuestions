--length of string using pointer.

#include<stdio.h>
#include<string.h>
int length(char *str){
    int count=strlen(str);
    return count;
}
int main(){
    char str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    length(str);
    return 0;
}


