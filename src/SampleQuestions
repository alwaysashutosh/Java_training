24.-----Input 3 strings x,y and z. Check if z is a perfect shuffle of x and y.???????????????????
#include <stdio.h>
#include <string.h>
#include <stdbool.h>

// Function to check if z is a perfect shuffle of x and y
bool isPerfectShuffle(const char *x, const char *y, const char *z) {
    int lenX = strlen(x);
    int lenY = strlen(y);
    int lenZ = strlen(z);

    // Length of z must be equal to sum of lengths of x and y
    if (lenZ != lenX + lenY) {
        return false;
    }

    int i = 0, j = 0, k = 0;

    // Try to match characters from x and y to z
    while (k < lenZ) {
        if (i < lenX && x[i] == z[k]) {
            i++;
        } else if (j < lenY && y[j] == z[k]) {
            j++;
        } else {
            return false;
        }
        k++;
    }

    return (i == lenX && j == lenY);
}

int main() {
    char x[100], y[100], z[200];

    printf("Enter string x: ");
    scanf("%s", x);

    printf("Enter string y: ");
    scanf("%s", y);

    printf("Enter string z: ");
    scanf("%s", z);

    if (isPerfectShuffle(x, y, z)) {
        printf("Yes, z is a perfect shuffle of x and y.\n");
    } else {
        printf("No, z is NOT a perfect shuffle of x and y.\n");
    }

    return 0;
}

