---print all substring of string.

#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i, j, k;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")] = '\0'; // Remove newline character

    int len = strlen(str);

    printf("All substrings of \"%s\":\n", str);
    for (i = 0; i < len; i++) {
        for (j = i; j < len; j++) {
            for (k = i; k <= j; k++) {
                printf("%c", str[k]);
            }
            printf("\n");
        }
    }

    return 0;
}


#include<stdio.h>
#include<string.h>
int main(){
    int str[100];
    printf("enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")]='\0';
    int i,j,k,n;
    n=strlen(str);
    for(i=0;i<n;i++){
        for(j=i;j<n;j++){
            for(k=i;k<=j;k++){
                printf("%c", str[k]);
            }
            printf("\n");
        }
    }
    return 0;
}


--------------------------------------------------------
--swap using pointers
#include<stdio.h>
void swap(int *a, int *b){
    int temp;
    temp=*a;
    *a=*b;
    *b=temp;
}
int main(){
    int a, b;
    printf("enter first number: ");
    scanf("%d", &a);
    printf("enter second number: ");
    scanf("%d", &b);
    swap(&a, &b);
    printf("swapped numbers are %d, %d", a, b);
    return 0;
}

