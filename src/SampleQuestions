#include<stdio.h>
int main(){
    int arr[100], i, n, end, j, pos;
    printf("enter no of ele: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    scanf("%d", &arr[i]);
    printf("enter pos to shift: ");
    scanf("%d", &pos);
    for(j=0;i<pos;j++){
        end=arr[n-1];
        for(i=n-1;i>0;i--){
            arr[i]=arr[i-1];
        }
        arr[0]=end;
    }
    for(i=0;i<n;i++){
        printf("%d", arr[i]);
    }
    return 0;
}

------Find the sum of elements at even indices.
#include<stdio.h>
int main(){
    int arr[100], i,n, sum=0;
    printf("enter no of ele: ");
    scanf("%d", &n);
    printf("enter %d elements: ", n);
    for(i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    for(i=0;i<n;i++){
        if(i%2==0){
            sum+=arr[i];
        }
    }
    printf("%d", sum);
    return 0;
}

