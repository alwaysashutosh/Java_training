1.--good number
#include<stdio.h>
#include<math.h>
#include<stdbool.h>
bool ispowertwo(int n){
    return (n>0 && (n&(n-1))==0);
}

int countGoodNum(int arr[], int n){
    int count=0;
    for(int i=0;i<n;i++){
        if(ispowertwo(arr[i])){
            count++;
        }
    }
    return count;
}
int main(){
    int arr[]={2,3,4,5,8};
    int n=sizeof(arr)/sizeof(arr[0]);
    int result=countGoodNum(arr, n);
    printf("%d", result);
    return 0;
}

------------------------END---------------------------

2.--magic array

void magicArr(int arr[], int n, int result[]){
    for(int i=0;i<n;i++){
        int count=0;
        for(int j=i+1;j<n;j++){
            if(arr[i]>arr[j]){
                count++;
            }
        }
        result[i]=count;
    }
}
int main(){
    int arr[]={5,4,3,1,2};
    int n=sizeof(arr)/sizeof(arr[0]);
    magicArr(arr, n, result);
    for(int i=0;i<n;i++){
        printf("%d\n", result[i]);
    }
    return 0;
}

------------------------END--------------------------

3.--maximum average of subarrays

#include<stdio.h>

int maxArr(int arr[], int n){
    int max=arr[0];
    for(int i=0;i<n;i++){
        if(arr[i]>max){
            max=arr[i];
        }
    }
    return max;
}
int main(){
    int arr[]={1,2,3,4,5};
    int n=sizeof(arr)/sizeof(arr[0]);
    int result=maxArr(arr,n);
    printf("%d", result);
    return 0;
}


--------------------END-----------------------

4.----weird array

#include<stdio.h>
void weirdArr(int arr[], int n, int result[]){
    int sum=0;
    for(int i=0; i<n; i++){
        sum+=arr[i];
    }
    for(int i=0;i<n;i++){
        int num=sum-arr[i];
        result[i]=num;
    }
}
int main(){
    int arr[]={1,2,3,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    weirdArr(arr, n, result);
    for(int i=0;i<n;i++){
        printf("%d", result[i]);
    }
    return 0;

}

-----------------------------------------------------

#include<stdio.h>
void weirdArr(int arr[], int n, int result[]){
    int sum=0;
    for(int i=0;i<n;i++){
        sum+=arr[i];
    }
    for(int i=0;i<n;i++){
        int num=sum-arr[i];
        result[i]=num;
    }
}

int main(){
    int arr[]={1,2,3,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    int result[n];
    weirdArr(arr, n, result);
    for(int i=0;i<n;i++){
        printf("%d", result[i]);
    }
    return 0;
}

------------------------END------------------------------

5.----invigilation--> invglato
#include<stdio.h>
#include<string.h>
void removeDup(char str[], int n, int freq[], char str1[]){
    for(int i=0;i<n;i++){
        freq[i]=-1;
    }
    for(int i=0;i<n;i++){
        int count=1;
        for(int j=i+1;j<n;j++){
            if(str[i]==str[j]){
                count++;
                freq[j]=0;
            }
        }      
        if(freq[i]!=0){
            freq[i]=count;
        }
    }
    int k=0;
    for(int i=0;i<n;i++){
        if(freq[i]!=0){
            str1[k++]=str[i];
        }
    }
    str1[k]='\0';
    
}
int main(){
    char str[]="invigilation";
    int n=strlen(str);
    char str1[100];
    int freq[100];
    removeDup(str, n, freq, str1);
    printf("%s", str1);
    return 0;
}


-----------------------END---------------------------------

6.--remove duplicates from string
#include <stdio.h>
#include <string.h>

void removeDuplicates(char str[], char result[]) {
    int seen[256] = {0}; // Track all ASCII characters
    int k = 0;
    for (int i = 0; str[i] != '\0'; i++) {
        unsigned char ch = (unsigned char)str[i];
        if (!seen[ch]) {
            seen[ch] = 1;
            result[k++] = str[i];
        }
    }
    result[k] = '\0';
}

int main() {
    char str[] = "invigilation";
    char result[100];
    removeDuplicates(str, result);
    printf("Result: %s\n", result);
    return 0;
}

------------------------END-------------------------

7.--check if all character in string are unique
#include <stdio.h>
#include <string.h>

int isUnique(char str[]) {
    int seen[256] = {0};
    for (int i = 0; str[i] != '\0'; i++) {
        unsigned char ch = (unsigned char)str[i];
        if (seen[ch]) {
            return 0; // Duplicate found
        }
        seen[ch] = 1;
    }
    return 1; // All characters are unique
}

int main() {
    char str[] = "abcdefg";
    if (isUnique(str)) {
        printf("All characters are unique.\n");
    } else {
        printf("Duplicates found.\n");
    }
    return 0;
}
