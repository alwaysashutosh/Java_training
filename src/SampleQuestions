#include<stdio.h>
#include<string.h>
#include<ctype.h>
char str1[1000];
void count(char str[]){
    int j=0;
    for(int i=0;str[i]!='\0';i++){
        if (!isspace(str[i])){
            str1[j++]=str[i];
        }
    }
}

int main(){
    char str[]=" hello   this is     me  how    about you  sham   ";
    count(str);
    printf("%s\n", str1);
    return 0;
}

--------------remove vowel
#include<stdio.h>
#include<string.h>
#include<ctype.h>
char str1[1000];
void count(char str[]){
    int j=0;
    for(int i=0;str[i]!='\0';i++){
        if (str[i]!='a' && str[i]!='e' && str[i]!='i' && str[i]!='o' && str[i]!='u'){
            str1[j++]=str[i];
        }
    }
}

int main(){
    char str[]=" hello   this is     me  how    about you  sham   ";
    count(str);
    printf("%s\n", str1);
    return 0;
}

